@model IEnumerable<Capstones.Models.Tourist>

@{
    ViewBag.Title = "index";
}

<h2>index</h2>

<p>
    <iframe width="600"
            height="450"
            frameborder="0" style="border:0"
            src="https://www.google.com/maps/embed/v1/place?key=@ViewBag.Key
    &q=@ViewBag.Interest, @ViewBag.CityTOSearch +@ViewBag.StateTOSearch" allowfullscreen>
    </iframe>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
            @Html.DisplayNameFor(Model=>Model.LastName)
        </th>
   
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName, item.LastName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
            </td>
        </tr>
    }

</table>
